# .env

# Flask settings
FLASK_ENV=development
FLASK_APP=main.py
SECRET_KEY=your-super-secret-key-here

# Redis settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Search engine settings
MAX_CONCURRENT_REQUESTS=20
REQUEST_TIMEOUT=30
MAX_PAGES=100
MAX_DEPTH=10

# Rate limiting
REQUESTS_PER_SECOND=2
BURST_SIZE=5

# Logging
LOG_LEVEL=INFO
